import sys


while True:
    n= int(sys.stdin.readline())
    if n == 0:
        break

    count = 0
    for i in range(n+1,n*2+1):
        if i > 1:
            check = True
            for j in range(2, int(i**0.5)+1):    
                if i%j==0:
                    check=False
                    break
            
            if check:
                count+=1
    print(count)

